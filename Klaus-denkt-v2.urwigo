function Staatsstreich()
	vUrsache = ""
	if vStaatsstreich then
		if    (vAPAufklaerung== 0) then vUrsache = "Aufklaerung = 0" 
		elseif (vAPLebensqualitaet== 0)  then vUrsache = vUrsache .. " Lebensqualitaet = 0"
		elseif (vAPBevoelkerung == 0)   then vUrsache = vUrsache .. " Bevoelkerung = 0"
		elseif (vAPBevoelkerung == 48)   then vUrsache = vUrsache .. " Bevoelkerung = max"
		elseif (vAPUmweltbelastung == 29)   then vUrsache = vUrsache .. " Umweltbelastung = max"
		elseif (vAPSanierung == 0)   then vUrsache = vUrsache .. " Sanierung = 0"
		elseif (vAPProduktion == 0)   then vUrsache = vUrsache .. " Produktion = 0"
		elseif (vAPProduktion == 29)   then vUrsache = vUrsache .. " Produktion = max"
		elseif (vAPVermehrungsrate == 0)   then vUrsache = vUrsache .. " Vermehrungsrate = 0"
		elseif (vAPVermehrungsrate == 29)   then vUrsache = vUrsache .. " Vermehrungsrate = max"
		elseif (vAPPolitik == -11))  then vUrsache = vUrsache .. " Politik = min" end
		return true
	else
